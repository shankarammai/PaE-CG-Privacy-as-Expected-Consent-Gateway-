{
  "manifest_version": 2,
  "name": "PaECG",
  "version": "0.1",
    "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
],
  "browser_action": {
    "default_title": "PaECG",
    "browser_style": true,
    "default_popup": "popup/index.html"
  }
}